{
  "manifest_version": 3,
  "name": "YouTube Downloader",
  "version": "1.0",
  "description": "Download YouTube videos using yt-dl",
  "permissions": ["activeTab", "scripting", "storage"],
  "host_permissions": ["http://localhost:5000/*"],
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  }
}
